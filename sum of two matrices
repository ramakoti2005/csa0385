#include<stdio.h>
int main()
{
	int r=3,c=3;
	int a[r][c]={{1,2,3},{4,5,6},{7,8,9}};
	int b[r][c]={{9,8,7},{6,5,4},{3,2,1}};
	int res[r][c];
	int i,j;
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			res[i][j]=a[i][j]+b[i][j];
		}
	}
		for(i=0;i<r;i++)
	    {
		  for(j=0;j<c;j++)
		  {
		  	printf("  %d  ",res[i][j]);
		  }
		  printf("\n");
	    }
}


output

  10    10    10  
  10    10    10
  10    10    10
